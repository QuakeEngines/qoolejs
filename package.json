{
  "name": "qoolejs",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "node bin/move-assets.js",
    "build": "npm run build:js && npm run build:sass",
    "build:js": "browserify --t [ babelify --presets [ es2015] ] src/js/index.js -o dist/js/app.js --debug",
    "build:sass": "node-sass --source-map dist/css/style.css.map --include-path=$(node bin/sass-paths.js) src/sass/style.sass dist/css/style.css",
    "build:pug": "pug --out dist --pretty src/pug",
    "watch": "npm run watch:js & npm run watch:sass & npm run livereload",
    "watch:js": "watchify -p browserify-hmr --t [ babelify --presets [ es2015] ] src/js/index.js  -o dist/js/app.js --debug",
    "watch:sass": "nodemon -w src/sass -e sass,scss -x 'npm run build:sass'",
    "watch:pug": "npm run build:pug -- --watch",
    "livereload": "livereload dist/**/*.css -d",
    "start": "npm run build && serve --path dist & npm run watch"
  },
  "author": "Alex Milanov",
  "license": "ISC",
  "dependencies": {
    "font-awesome": "^4.6.3",
    "iblokz-data": "^1.0.2",
    "iblokz-snabbdom-helpers": "^1.0.0",
    "lodash": "^4.13.1",
    "rx": "^4.1.0",
    "three": "^0.78.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "browserify-hmr": "^0.3.5",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.6.0",
    "fs-extra": "^0.30.0",
    "livereload": "^0.6.2",
    "node-sass": "^3.7.0",
    "node-serve": "0.0.3",
    "nodemon": "^1.9.2",
    "pug-cli": "^1.0.0-alpha6",
    "watchify": "^3.8.0"
  }
}
